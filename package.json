  "scripts": {
    "build": "node webpack/production/build.js",
    "start": "node webpack/development/webserver.js",
    "spell": "cspell 'src/**/*.{txt,md,js,ts,tsx,jsx}' --gitignore",
    "start:chrome": "chromium --new-window --load-extension=./build/ --user-data-dir=\"/tmp/${RANDOM}\"",
    "start:opera": "opera --new-window --load-extension=./build/ --user-data-dir=\"/tmp/${RANDOM}\"",
    "start:edge": "edge --new-window --load-extension=./build/ --user-data-dir=\"/tmp/${RANDOM}\"",
    "start:firefox": "web-ext run --source-dir ./build-browser/",
    "tags": "git tag --sort=v:refname --format '%(refname:strip=2)' | tail -n 20"
  },
  "browserslist": "defaults",
